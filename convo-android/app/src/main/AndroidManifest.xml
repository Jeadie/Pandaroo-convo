<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.jam.pandaroo_convo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".InitialSurveyActivity" />
        <activity
            android:name=".DiscussionActivity"
            android:label="@string/title_activity_discussion"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".FinishedChatActivity"
            android:label="@string/title_activity_finished_chat"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".PostSurveyActivity"
            android:label="PostSurveyActivity"
            android:theme="@style/AppTheme" />
        />
        <activity
            android:name=".ProfileActivity"
            android:label="@string/title_activity_profile"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".FindGroupActivity"
            android:label="@string/title_activity_find_group"
            android:theme="@style/AppTheme"></activity>
    </application>

</manifest>